{
  "manifest_version": 2,
  "name": "github:status",
  "version": "0.0.1",
  "permissions": [
    "https://status.github.com/api"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.9.1.min.js",
      "js/moment.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon-default32.png",
    "default_title": "github:status"
  },
  "icons": {
    "32": "img/icon-green32.png"
  }
}
